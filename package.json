{
  "name": "cypress-javascript-pom",
  "version": "1.0.0",
  "description": "Cypress using Cucumber with Page Objects",
  "main": "index.js",
  "scripts": {
    "test:open": "cypress open",
    "test:run": "cypress run && node ./cypress/cucumber-html-report.js",
    "test:report": "node ./cypress/cucumber-html-report.js"
  },
  "keywords": [
    "Cypress",
    "Cucumber",
    "BDD",
    "Gherkin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/athan3350/cypress-javascript-POM.git"
  },
  "author": "Sebastian Suarez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/athan3350/cypress-javascript-POM/issues"
  },
  "homepage": "https://github.com/athan3350/cypress-javascript-POM#readme",
  "devDependencies": {
    "cypress": "^9.7.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "multiple-cucumber-html-reporter": "^1.20.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  }
}
